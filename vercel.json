{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/generate-commercial-proposal.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
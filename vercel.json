{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "env": {
    "TELEGRAM_BOT_TOKEN": "@telegram_bot_token",
    "FIREBASE_PROJECT_ID": "paper-control-6bce2"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}